{
  "name": "@getcommunity/roots",
  "description": "Get Community Roots",
  "version": "0.0.0",
  "author": "@GetCommunity <ryan@getcommunity.com> (https://github.com/GetCommunity)",
  "contributors": [
    "Joey Grable <joey@getcommunity.com> (https://joeygrable.com)"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@changesets/cli": "^2.27.7",
    "@getcommunity/config-prettier": "workspace:*",
    "@testing-library/jest-dom": "^6.4.8",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/ui": "^2.0.4",
    "autoprefixer": "^10.4.19",
    "babel-preset-solid": "^1.8.18",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sass": "^1.77.8",
    "solid-devtools": "0.30.1",
    "syncpack": "13.0.0",
    "tsup": "^8.2.2",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.3.4",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.0.4"
  },
  "keywords": [],
  "license": "MIT",
  "packageManager": "pnpm@8.15.6",
  "prettier": {
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 88,
    "tabWidth": 2,
    "useTabs": false
  },
  "private": true,
  "repository": "getcommunity/roots.git",
  "scripts": {
    "build": "turbo build",
    "ci:changeset": "changeset",
    "ci:release": "pnpm build && changeset publish",
    "ci:version": "changeset version && pnpm i --no-frozen-lockfile && git add .",
    "clean": "turbo clean && rm -rf node_modules && rm -rf .turbo",
    "dev": "turbo dev",
    "dev:gcui": "turbo dev --filter=\"./packages/gcui-tailwind\" --filter=\"./packages/gcui\"",
    "dev:gcui-docs": "turbo dev --filter=\"./apps/gcui-docs\"",
    "format": "turbo format",
    "graph:build:all": "turbo run build --graph --dry | dot -Tpng -o./assets/dependency-graphs/turbo_graph-build-all.png",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}