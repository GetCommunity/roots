{
  "name": "@getcommunity/roots",
  "description": "Get Community Roots",
  "version": "0.0.0",
  "author": "@GetCommunity <ryan@getcommunity.com> (https://github.com/GetCommunity)",
  "contributors": [
    "Joey Grable <joey@getcommunity.com> (https://joeygrable.com)"
  ],
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@changesets/cli": "^2.27.10",
    "@getcommunity/config-prettier": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.17.6",
    "@eslint/js": "9.11.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "babel-preset-solid": "^1.9.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.15.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sass": "^1.81.0",
    "solid-devtools": "0.30.1",
    "syncpack": "13.0.0",
    "tsup": "^8.3.5",
    "turbo": "^2.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.1.5"
  },
  "keywords": [],
  "license": "MIT",
  "packageManager": "pnpm@8.15.6",
  "prettier": {
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 88,
    "tabWidth": 2,
    "useTabs": false
  },
  "private": true,
  "repository": "getcommunity/roots.git",
  "scripts": {
    "build": "turbo build",
    "ci:changeset": "changeset",
    "ci:release": "pnpm build && changeset publish",
    "ci:version": "changeset version && pnpm i --no-frozen-lockfile && git add .",
    "clean": "turbo clean && rm -rf node_modules && rm -rf .turbo",
    "dev": "turbo dev",
    "dev:gcui": "turbo dev --filter=\"./packages/gcui-tailwind\" --filter=\"./packages/gcui\"",
    "dev:gcui-docs": "turbo dev --filter=\"./apps/gcui-docs\"",
    "format": "turbo format",
    "graph:build:all": "turbo run build --graph --dry | dot -Tpng -o./assets/dependency-graphs/turbo_graph-build-all.png",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}