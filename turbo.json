{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"format": {
			"cache": false
		},
		"lint": {
			"cache": false
		},
		"test": {
			"dependsOn": []
		},
		"typecheck": {
			"cache": false
		},
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		}
	}
}